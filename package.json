{
  "name": "toonfetch",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "packageManager": "pnpm@10.20.0",
  "exports": {
    "./ory": {
      "types": "./dist/ory.d.ts",
      "import": "./dist/ory.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm update:exports && tsdown && pnpm copy:types && pnpm add:refs",
    "automd": "automd",
    "generate:types": "tsx scripts/generate-types.ts",
    "update:exports": "tsx scripts/update-exports.ts",
    "copy:types": "tsx scripts/copy-types.ts",
    "add:refs": "tsx scripts/add-type-references.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm -r test",
    "test:types": "tsc --noEmit",
    "release": "bumpp -r"
  },
  "dependencies": {
    "apiful": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.9.2",
    "automd": "^0.4.2",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.0",
    "openapi-typescript": "^7.10.1",
    "tsdown": "^0.15.12",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  }
}
